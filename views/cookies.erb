<link rel="stylesheet" href="goods.css">
  <% @@cookies.each do |cookie| %>
    <div class="pics">
    <img src="<%= cookie.image %>">
    </div>
<div class="info">

    <h2 id ="name"><%= cookie.name %> Cookie</h2>
    <h2 id ="allerg"><%= cookie.allergens %> Cookie</h2>
    <p id ="desc"><%= cookie.description %></p>
    <p id ="price">$<%= cookie.price %></p>
</div>
    <% end %>
